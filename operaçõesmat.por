programa {
  funcao inicio() {
    //operações matématicas
    inteiro valor1, valor2

    escreva("Digite o valor 1: ")
    leia(valor1)
    escreva("Digite o valor 2:")
    leia(valor2)

    escreva("o resultado da 4 opreações: ")
    escreva("\nSoma: ", valor1 + valor2)
    escreva("\n subtração: ", valor1 - valor2)
    escreva("\nMultiplicação: ", valor1 * valor2)
    escreva("\nDivisão: ", valor1 / valor2)
    
  }
}
